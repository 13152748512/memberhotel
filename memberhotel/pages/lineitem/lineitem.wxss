.pages{
  background: #fff !important;
  color:#252525;
}
.dishes,.remark{
  width:100%;
  padding:0rpx 30rpx;
  box-sizing: border-box;
}
.dishestitle, .payment,.remarktitle,.disheslist{
  width:100%;
}
.payment,.dishestitle,.remarktitle{
  height:80rpx; 
  line-height: 80rpx; 
}
.dishestitle,.remarktitle{
  font-size: 28rpx;
  border-bottom:1px solid #EFEFEF;
}
.disheslist{
  padding:20rpx 15rpx;
  box-sizing: border-box;
  border-bottom: 1px solid #EFEFEF;
  display: flex;
  justify-content: space-between;
}
.dishName{
  display: inline-block;
  width:50%;
}
.payment text{
  margin-right: 80rpx;
}
.paymentAmount{
  margin-right: 0rpx !important;
}
.remarktitle{
  margin-bottom:12rpx;
}
.remarkMessage view{
  width:100%;
  height: 60rpx;
  line-height: 60rpx;
}